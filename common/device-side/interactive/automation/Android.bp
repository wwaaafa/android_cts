android_app {
    name: "InteractiveAutomation",
    srcs: [
        "src/main/java/**/*.java"
    ],
    static_libs: [
        "InteractiveCommon",
        "Nene",
        "androidx.test.uiautomator_uiautomator",
    ],
    manifest: "src/main/AndroidManifest.xml",
    min_sdk_version: "29",
    dex_preopt: {
        enabled: false,
    },
    optimize: {
            enabled: false,
        },
    platform_apis: true
}